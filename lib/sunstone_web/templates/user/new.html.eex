<%= form_for @changeset, Routes.user_path(@conn, :create), fn f -> %>
  <label>
    Email: <%= text_input f, :email %>
    <%= error_tag f, :email %>
  </label>

  <label>
    
    Password: <%= text_input f, :password, type: "password"%>
    <%= error_tag f, :password %>
  </label>
    <label>
    Retype Password: <%= text_input f, :retype_password, type: "password"%>
  </label>

  <%= submit "Submit" %>
<% end %>